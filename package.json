{
  "name": "natours",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/homepageStyle.css -w",
    "devserver": "live-server",
    "start": "node server.js & npm-run-all --parallel devserver watch:sass",
    "start:prod": "SET NODE_ENV=production & nodemon server.js --parallel devserver watch:sass",
    "debug": "ndb server.js",
    "compile:sass": "node-sass sass/main.scss public/css/homepageStyle.comp.css",
    "concat:css": "concat -o public/css/homepageStyle.concat.css public/css/icon-font.css public/css/homepageStyle.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/css/homepageStyle.concat.css -o public/css/homepageStyle.prefix.css",
    "compress:css": "node-sass public/css/homepageStyle.prefix.css public/css/homepageStyle.css --output-homepageStyle compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
  },
  "author": "Sergiu Tont",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jimp": "^0.10.2",
    "jsonwebtoken": "^8.5.1",
    "live-server": "^1.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "pug": "^2.0.4",
    "slugify": "^1.4.0",
    "stripe": "^8.44.0",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "ndb": "^1.1.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
