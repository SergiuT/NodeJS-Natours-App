{
  "name": "natours",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/homepageStyle.css -w",
    "devserver": "live-server",
    "start": "nodemon server.js & npm-run-all --parallel devserver watch:sass",
    "start:prod": "SET NODE_ENV=production & nodemon server.js --parallel devserver watch:sass",
    "debug": "ndb server.js",
    "compile:sass": "node-sass sass/main.scss public/css/homepageStyle.comp.css",
    "concat:css": "concat -o public/css/homepageStyle.concat.css public/css/icon-font.css public/css/homepageStyle.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/css/homepageStyle.concat.css -o public/css/homepageStyle.prefix.css",
    "compress:css": "node-sass public/css/homepageStyle.prefix.css public/css/homepageStyle.css --output-homepageStyle compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Sergiu Tont",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "live-server": "^1.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "pug": "^2.0.4",
    "slugify": "^1.3.5",
    "validator": "^11.1.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "ndb": "^1.1.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
